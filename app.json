{
  "expo": {
    "sdkVersion": "28.0.0",
    "icon": "./assets/eastern-logo.png",
    "name": "Dictionary",
    "slug": "eastern",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "packagerOpts": {
      "assetExts": [
        "ttf", "db"
      ]
    },
    "androidStatusBarColor": "#334393",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#334393"
    },
    "splash": {
      "backgroundColor": "#FFFFFF",
      "resizeMode": "contain",
      "image": "./assets/splashscreen.png"
    },
    "assetBundlePatterns": [
      "assets/database/*",
      "assets/eastern-logo.png",
      "assets/eastern-logo-small.png",
      "assets/splashscreen.png"
    ],
    "ios": {
      "bundleIdentifier": "com.hashtagracoon.eastern"
    },
    "android": {
      "package": "com.hashtagracoon.eastern",
      "permissions": []
    }
  }
}
